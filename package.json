  "dependencies": {
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "googleapis": "^39.2.0",
    "mongoose": "^5.11.8",
    "node-cron": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "nodemon": "^2.0.7"
  }
}
